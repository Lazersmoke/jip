sudo mkdir -p /usr/local/jip/runs /usr/local/jip/depends /usr/local/jip/userruns /usr/local/jip/package-info/$1/ /tmp/jiptemp/
tar -C /tmp/jiptemp/ -xf $1
sudo cp /tmp/jiptemp/runs/* /usr/local/jip/runs/ -r
sudo cp /tmp/jiptemp/depends/* /usr/local/jip/depends/ -r
sudo cp /tmp/jiptemp/userruns/* /usr/local/jip/userruns/ -r
sudo cp /tmp/jiptemp/package-info/* /usr/local/jip/package-info/$1/ -r
sudo rm /tmp/jiptemp/ -r
